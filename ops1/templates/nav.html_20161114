<script type="text/javascript">
function show()
{
	var vshowOrHide = document.getElementById("showOrHide");
	vshowOrHide.className = 'glyphicon glyphicon-chevron-right';
	var vrunsalt = document.getElementById("runsalt");
	vrunsalt.className = 'glyphicon glyphicon-chevron-right';
}
function runsalt_f()
{
	var vzxzyul = document.getElementById("zxzyul");
	var vrunsalt = document.getElementById("runsalt");
	var vdt12 = document.getElementById("dt12");
	var vdt22 = document.getElementById("dt22");
	var vdt32 = document.getElementById("dt32");
	var vdt42 = document.getElementById("dt42");
	var vdt52 = document.getElementById("dt52");
	if(vzxzyul.className == "ctt1"){
		vzxzyul.className = "ctt0";
		vrunsalt.className = "glyphicon glyphicon-chevron-right";
		vdt12.className = 'tbl0';
		vdt22.className = 'tbl0';
		vdt32.className = 'tbl0';
		vdt42.className = 'tbl0';
		vdt52.className = 'tbl0';
	} else {
		vzxzyul.className = "ctt1";
		vrunsalt.className = "glyphicon glyphicon-chevron-down";
	};
}
function showhide()
{
//	var news = document.getElementById("content");
//	if(typeof(news) != "undefined" ){
//		news.style.display = (news.style.display.toLowerCase()=="none") ? "":"none";
//		this.innerHTML = (news.style.display.toLowerCase()=="none") ? "-资产信息" : "+资产信息";
//	}
	var vctt = document.getElementById("content");
	var vshowOrHide = document.getElementById("showOrHide");
	var vdt1 = document.getElementById("dt1");
	var vdt2 = document.getElementById("dt2");
	var vdt3 = document.getElementById("dt3");
	var vdt4 = document.getElementById("dt4");
	if(vctt.className == 'ctt1'){
		vctt.className = 'ctt0';
		vshowOrHide.className = 'glyphicon glyphicon-chevron-right';
		vdt1.className = 'tbl0';
		vdt2.className = 'tbl0';
		vdt3.className = 'tbl0';
		vdt4.className = 'tbl0';
	} else {
		vctt.className = 'ctt1'
		vshowOrHide.className = 'glyphicon glyphicon-chevron-down';
	};
}

